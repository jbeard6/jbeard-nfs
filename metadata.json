      "operatingsystemrelease":[ "12.04", "14.04" ]
    }
  ],
  "dependencies": [
    {"name":"jbeard/portmap","version_requirement":">= 0.1.11 < 0.2.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.0.0 < 5.0.0"}
  ]
}
