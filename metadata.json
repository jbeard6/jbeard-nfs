{
  "name": "jbeard-nfs",
  "version": "0.2.1",
  "author": "jbeard",
  "summary": "NFS Client and Server",
  "license": "Apache-2.0",
  "source": "https://github.com/jbeard6/jbeard-nfs",
  "project_page": "https://github.com/jbeard6/jbeard-nfs",
  "issues_url": "https://github.com/jbeard6/jbeard-nfs/issues",
